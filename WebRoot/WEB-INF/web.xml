<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>vlib_judge</display-name>
  
  <servlet>
    <servlet-name>SaveRuleAction</servlet-name>
    <servlet-class>vlib.action.SaveRuleAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestAction</servlet-name>
    <servlet-class>vlib.action.TestAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>JudgeAction</servlet-name>
    <servlet-class>vlib.action.JudgeAction</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>SaveRuleAction</servlet-name>
    <url-pattern>/saverule</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestAction</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JudgeAction</servlet-name>
    <url-pattern>/judge</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>submit.html</welcome-file>
  </welcome-file-list>
</web-app>